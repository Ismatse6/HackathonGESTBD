@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dbo:  <http://dbpedia.org/ontology/> .
@prefix dbr:  <http://dbpedia.org/resource/> .
@prefix upm:  <http://upm.es/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

###############################################################################
# Ontology header
###############################################################################
upm:  rdf:type owl:Ontology ;
      rdfs:comment "Ontología UPM - clases, propiedades de datos y objetos"@es .

###############################################################################
# Classes
###############################################################################
upm:Escuela              rdf:type owl:Class ;
                         rdfs:subClassOf dbo:EducationalInstitution ;
                         rdfs:label "Escuela"@es .

upm:Titulacion           rdf:type owl:Class ;
                         rdfs:subClassOf dbo:Academic_degree ;
                         rdfs:label "Titulación"@es .

upm:Asignatura           rdf:type owl:Class ;
                         rdfs:subClassOf dbo:Course ;
                         rdfs:label "Asignatura"@es .

upm:Profesor             rdf:type owl:Class ;
                         rdfs:subClassOf dbo:Person ;
                         rdfs:label "Profesor"@es .

upm:RecursoBibliografico rdf:type owl:Class ;
                         rdfs:subClassOf dbo:Book ;
                         rdfs:label "Recurso bibliográfico"@es .

###############################################################################
# Datatype properties (literales)
###############################################################################
upm:nombre rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Escuela , upm:Titulacion , upm:Asignatura , upm:Profesor ;
           rdfs:range xsd:string ;
           rdfs:label "nombre"@es ;
           owl:equivalentProperty foaf:name .

upm:codigo rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Titulacion , upm:Asignatura , upm:Escuela ;
           rdfs:range xsd:integer ;
           rdfs:label "código"@es .

upm:codigoTitulacion rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Titulacion ;
           rdfs:range xsd:string ;
           rdfs:label "códigoTitulación"@es .

upm:creditosECTS rdf:type owl:DatatypeProperty ;
                 rdfs:domain upm:Asignatura ;
                 rdfs:range xsd:integer ;
                 rdfs:label "créditos ECTS"@es .

upm:semestre rdf:type owl:DatatypeProperty ;
             rdfs:domain upm:Asignatura ;
             rdfs:range xsd:string ;
             rdfs:label "semestre"@es .

upm:idioma rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Asignatura ;
           rdfs:range xsd:string ;
           rdfs:label "idioma"@es .

upm:correo rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Profesor ;
           rdfs:range xsd:string ;
           rdfs:label "correo"@es .

upm:titulo rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:RecursoBibliografico ;
           rdfs:range xsd:string ;
           rdfs:label "título"@es .

upm:autor rdf:type owl:DatatypeProperty ;
          rdfs:domain upm:RecursoBibliografico ;
          rdfs:range xsd:string ;
          rdfs:label "autor"@es .

upm:tipo rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:Titulacion ;
           rdfs:range xsd:string ;
           rdfs:label "tipo_estudio"@es .
  
upm:direccionURL rdf:type owl:DatatypeProperty ;
           rdfs:domain upm:RecursoBibliografico ;
           rdfs:range xsd:string ;
           rdfs:label "direccion_url"@es .  

###############################################################################
# Object properties (relaciones entre recursos)
###############################################################################
upm:tieneEscuela rdf:type owl:ObjectProperty ;
                 rdfs:domain dbo:University ;
                 rdfs:range upm:Escuela ;
                 rdfs:label "tieneEscuela"@es .

upm:imparteTitulacion rdf:type owl:ObjectProperty ;
                      rdfs:domain upm:Escuela ;
                      rdfs:range upm:Titulacion ;
                      rdfs:label "imparteTitulación"@es .

upm:incluyeAsignatura rdf:type owl:ObjectProperty ;
                      rdfs:domain upm:Titulacion ;
                      rdfs:range upm:Asignatura ;
                      rdfs:label "incluyeAsignatura"@es .

upm:tieneProfesor rdf:type owl:ObjectProperty ;
                  rdfs:domain upm:Asignatura ;
                  rdfs:range upm:Profesor ;
                  rdfs:label "tieneProfesor"@es .

upm:perteneceAEscuela rdf:type owl:ObjectProperty ;
                      rdfs:domain upm:Profesor ;
                      rdfs:range upm:Escuela ;
                      rdfs:label "perteneceAEscuela"@es .
                      
upm:tieneRecursoBibliografico rdf:type owl:ObjectProperty ;
                              rdfs:domain upm:Asignatura ;
                              rdfs:range upm:RecursoBibliografico ;
                              rdfs:label "tieneRecursoBibliografico"@es .

upm:asociadoA rdf:type owl:ObjectProperty ;
              rdfs:domain upm:RecursoBibliografico ;
              rdfs:range upm:Asignatura ;
              rdfs:label "asociadoA"@es .